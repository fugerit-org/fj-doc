  <admeta:admSection>
    <admeta:name>meta_fixed</admeta:name>
    <admeta:description>Properties specific to fixed size formats (like PDF)</admeta:description>
    <admeta:admMetaInfo>
      <admeta:name>margins</admeta:name>
      <admeta:description>Document page margins, in pixels, in the format LEFT;RIGHT;TOP;BOTTOM</admeta:description>
      <admeta:defaultValue>10;10;10;10</admeta:defaultValue>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>page-width</admeta:name>
      <admeta:description>Page width, in cm, for example 21cm for A4 portrait or 29.7cm for A4 landscape</admeta:description>
      <admeta:defaultValue>21cm</admeta:defaultValue>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>page-height</admeta:name>
      <admeta:description>Page height, in cm, for example 29.7cm for A4  or 21cm for A4 landscape</admeta:description>
      <admeta:defaultValue>29.7cm</admeta:defaultValue>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>page-orient</admeta:name>
      <admeta:description>Page orientation, 'vertical' for portrait or 'horizontal' for landscape</admeta:description>
      <admeta:defaultValue>vertical</admeta:defaultValue>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-openpdf(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-itext(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
  </admeta:admSection>
  
  <admeta:admSection>
  	<admeta:name>meta_xls</admeta:name>
    <admeta:description>Properties specific to spreadsheet format (XLS/XLSX)</admeta:description>
    <admeta:admMetaInfo>
      <admeta:name>excel-table-id</admeta:name>
      <admeta:description>This property is required to generate spreadsheet format. It should be a list of ';' separated element. 
Each element is a key=value set, where 'key' is the id of a table in the document which will be the content of a new sheet, 
the 'value' is the name of the sheet to be created. i.e. 'table-1=sheet1;table-2=sheet2'.</admeta:description>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>xls</admeta:supportedType>
      <admeta:supportedType>xlsx</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-poi(xls)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-poi(xlsx)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>excel-try-autoresize</admeta:name>
      <admeta:description>If set to 'true', a column auto resize will be tried on the created sheets.</admeta:description>
      <admeta:defaultValue>false</admeta:defaultValue>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>xls</admeta:supportedType>
      <admeta:supportedType>xlsx</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-poi(xls)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-poi(xlsx)</admeta:supportedHandler>
    </admeta:admMetaInfo>
     <admeta:admMetaInfo>
      <admeta:name>excel-fail-on-autoresize-error</admeta:name>
      <admeta:description>If both this property and 'excel-try-autoresize' are set to true, the generation will fail in case of autoresize errors.</admeta:description>
      <admeta:defaultValue>false</admeta:defaultValue>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>xls</admeta:supportedType>
      <admeta:supportedType>xlsx</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-poi(xls)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-poi(xlsx)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>excel-ignore-format</admeta:name>
      <admeta:description>If set to 'true', column format will be ignored and all content will be rendered as string.</admeta:description>
      <admeta:defaultValue>false</admeta:defaultValue>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>xls</admeta:supportedType>
      <admeta:supportedType>xlsx</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-poi(xls)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-poi(xlsx)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>excel-template</admeta:name>
      <admeta:description>Path to a template to use as a base for the output. If the prefix is 'file://' will load from file system. If prefix is 'cl://' will load from classlaoder.</admeta:description>
      <admeta:defaultValue>false</admeta:defaultValue>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>xls</admeta:supportedType>
      <admeta:supportedType>xlsx</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-poi(xls)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-poi(xlsx)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-jxl(xls) [deprecated]</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>excel-width-multiplier</admeta:name>
      <admeta:description>A multiplier for zooming the width of the generated excel. Base value is '256'</admeta:description>
      <admeta:defaultValue>256</admeta:defaultValue>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>xls</admeta:supportedType>
      <admeta:supportedType>xlsx</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-jxl(xls) [deprecated]</admeta:supportedHandler>
    </admeta:admMetaInfo>
  </admeta:admSection>

  <admeta:admSection>
  	<admeta:name>meta_csv</admeta:name>
    <admeta:description>Properties specific to CSV format</admeta:description>
    <admeta:admMetaInfo>
      <admeta:name>csv-table-id</admeta:name>
      <admeta:description>This property is required to generate the CSV format. It should be a single id of a table contained in the source document to be rendered as CSV.</admeta:description>
      <admeta:since>1.4.5</admeta:since>
      <admeta:supportedType>csv</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-opencsv(csv)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>csv-separator</admeta:name>
      <admeta:description>The character separator to be used for the CSV.</admeta:description>
      <admeta:defaultValue>,</admeta:defaultValue>
      <admeta:since>3.1.8</admeta:since>
      <admeta:supportedType>csv</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-opencsv(csv)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>csv-line-end</admeta:name>
      <admeta:description>The line terminator for the CSV.</admeta:description>
      <admeta:defaultValue>\n</admeta:defaultValue>
      <admeta:since>3.1.8</admeta:since>
      <admeta:supportedType>csv</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-opencsv(csv)</admeta:supportedHandler>
    </admeta:admMetaInfo>
  </admeta:admSection>

</admeta:autodocMeta>
