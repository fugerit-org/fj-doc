  <admeta:admSection>
    <admeta:name>meta_general</admeta:name>
    <admeta:description>Properties for generic metadata</admeta:description>
    <admeta:admMetaInfo>
      <admeta:name>doc-title</admeta:name>
      <admeta:description>Set the document title, should be usually supported in document formats like html or pdf</admeta:description>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>doc-author</admeta:name>
      <admeta:description>Set the document author, should be usually supported in document formats like html or pdf</admeta:description>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>doc-creator</admeta:name>
      <admeta:description>Set the document creator tool (es. Venus Doc), should be usually supported in document formats like html or pdf</admeta:description>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>doc-language</admeta:name>
      <admeta:description>Set the document language, should be usually supported in document formats like html or pdf</admeta:description>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
     <admeta:admMetaInfo>
      <admeta:name>doc-subject</admeta:name>
      <admeta:description>Set the document subject, should be usually supported in document formats like html or pdf</admeta:description>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
     <admeta:admMetaInfo>
      <admeta:name>doc-version</admeta:name>
      <admeta:description>A custom property to track meta model version</admeta:description>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>default-table-padding</admeta:name>
      <admeta:description>Set default table padding, default '-1' means 'unset' (shoud be inherited by all cells, in HTML it is handled by 'padding')</admeta:description>
      <admeta:defaultValue>-1</admeta:defaultValue>
      <admeta:since>1.5.0</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedType>xls</admeta:supportedType>
      <admeta:supportedType>xlsx</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-poi(xls)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-poi(xlsx)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-openpdf(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-itext(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>default-table-spacing</admeta:name>
      <admeta:description>Set default table spacing, default '-1' means 'unset' (shoud be inherited by all cells, in HTML it is handled by 'margin')</admeta:description>
      <admeta:defaultValue>-1</admeta:defaultValue>
      <admeta:since>1.5.0</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedType>xls</admeta:supportedType>
      <admeta:supportedType>xlsx</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-poi(xls)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-poi(xlsx)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-openpdf(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-itext(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>default-cell-border-width</admeta:name>
      <admeta:description>Set default cell border width for all table cells, default '-1' means 'unset'</admeta:description>
      <admeta:defaultValue>-1</admeta:defaultValue>
      <admeta:since>1.5.0</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedType>xls</admeta:supportedType>
      <admeta:supportedType>xlsx</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-poi(xls)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-poi(xlsx)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-openpdf(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-itext(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>table-border-collapse</admeta:name>
      <admeta:description>Set the default table-border-collapse mode, possible values are 'separate' or 'collapse'. (note that this is a document scoped property, all tables in documnent will be affected)</admeta:description>
      <admeta:defaultValue>separate</admeta:defaultValue>
      <admeta:since>3.2.2</admeta:since>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>default-font-name</admeta:name>
      <admeta:description>Set the default font name</admeta:description>
      <admeta:defaultValue>helvetica</admeta:defaultValue>
      <admeta:since>1.4.5</admeta:since>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedType>rtf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-openpdf-ext(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-openrtf-ext(rtf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>default-font-size</admeta:name>
      <admeta:description>Set the default font size</admeta:description>
      <admeta:defaultValue>10</admeta:defaultValue>
      <admeta:since>1.4.5</admeta:since>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedType>rtf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-openpdf-ext(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-openrtf-ext(rtf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>default-font-style</admeta:name>
      <admeta:description>Set the default font style</admeta:description>
      <admeta:defaultValue>normal</admeta:defaultValue>
      <admeta:since>1.4.5</admeta:since>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedType>rtf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-fop(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-openpdf-ext(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-openrtf-ext(rtf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>suppress-wrong-type-error</admeta:name>
      <admeta:description>If set to 'a' or 'true' it will set suppress some type error.</admeta:description>
      <admeta:defaultValue>false</admeta:defaultValue>
      <admeta:since>8.11.5</admeta:since>
      <admeta:supportedType>pdf</admeta:supportedType>
      <admeta:supportedType>rtf</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-mod-openpdf-ext(pdf)</admeta:supportedHandler>
      <admeta:supportedHandler>fj-doc-mod-openrtf-ext(rtf)</admeta:supportedHandler>
    </admeta:admMetaInfo>
  </admeta:admSection>

  <admeta:admSection>
    <admeta:name>meta_html</admeta:name>
    <admeta:description>Properties specific to HTML format</admeta:description>
    <admeta:admMetaInfo>
      <admeta:name>html-css-link</admeta:name>
      <admeta:description>Link to css for the html document</admeta:description>
      <admeta:since>1.4.5</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>html-css-style</admeta:name>
      <admeta:description>Inline CSS style definition to be added to the document</admeta:description>
      <admeta:since>1.4.5</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>html-add-to-head</admeta:name>
      <admeta:description>Custom HTML code to be added directly in the head section</admeta:description>
      <admeta:since>1.0.0</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
      <admeta:supportedHandler>fj-doc-freemarker(html)</admeta:supportedHandler>
    </admeta:admMetaInfo>
    <admeta:admMetaInfo>
      <admeta:name>html-charset</admeta:name>
      <admeta:description>Add meta charset to head</admeta:description>
      <admeta:since>3.4.5</admeta:since>
      <admeta:supportedType>html</admeta:supportedType>
